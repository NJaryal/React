{
    "name": "reactapp",
    "version": "1.0.0",
    "description": "React + Webpack + Babel",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --mode development --open --hot",
        "build": "webpack --mode production",
        "test:watch": "jest --watchAll",
        "cypress": "cypress open"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NJaryal/ReactApp.git"
    },
    "author": "Nitin Jaryal",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/NJaryal/ReactApp/issues"
    },
    "homepage": "https://github.com/NJaryal/ReactApp#readme",
    "dependencies": {
        "axios": "^0.19.0",
        "babel-polyfill": "^6.26.0",
        "bootstrap": "^4.3.1",
        "classnames": "^2.2.6",
        "history": "^4.9.0",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-bootstrap": "^1.0.0-beta.9",
        "react-dom": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "redux": "^4.0.1",
        "redux-saga": "^1.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.4.5",
        "@babel/plugin-proposal-class-properties": "^7.4.4",
        "@babel/preset-env": "^7.4.5",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.6",
        "check-prop-types": "^1.1.2",
        "css-loader": "^3.0.0",
        "cypress": "^3.4.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "enzyme-to-json": "^3.3.5",
        "eslint": "^5.16.0",
        "eslint-config-airbnb": "^17.1.1",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.14.2",
        "html-webpack-plugin": "^3.2.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.8.0",
        "jest-resolve": "^24.8.0",
        "redux-mock-store": "^1.5.3",
        "style-loader": "^0.23.1",
        "url-loader": "^2.0.0",
        "webpack": "^4.33.0",
        "webpack-cli": "^3.3.4",
        "webpack-dev-server": "^3.7.1"
    },
    "jest": {
        "setupFilesAfterEnv": [
            "<rootDir>__tests__/setup/setupEnzyme.js"
        ],
        "testPathIgnorePatterns": [
            "<rootDir>/__tests__/setup/"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/*.js",
            "\\.(css|less)$": "identity-obj-proxy",
            "src(.*)$": "<rootDir>/src/$1",
            "Components(.*)$": "<rootDir>/src/Components/$1"
        },
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    }
}